// adds custom checkout page notes after order notes

// custom notes can also be inserted before order notes using 
// add_action( 'woocommerce_before_order_notes', 'notice_custom' );
 
add_action( 'woocommerce_after_order_notes', 'notice_custom' );

function notice_custom() {

echo '<p>Enter your custom checkout message here</p>';

}
